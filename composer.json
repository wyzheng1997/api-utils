{
    "name": "wyzheng/api-utils",
    "license": "MIT",
    "type": "library",
    "version": "v1.0.1",
    "keywords": [
        "api",
        "utils",
        "laravel"
    ],
    "description": "一个适用于laravel开发API服务的工具包",
    "autoload": {
        "psr-4": {
            "Ugly\\ApiUtils\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "wyzheng",
            "email": "wyzheng1997@163.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ugly\\ApiUtils\\ServiceProvider"
            ]
        }
    }
}
